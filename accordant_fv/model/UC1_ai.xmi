      qAttribute="PERFORMANCE"
      stimulus="Stiml"
      environment="Env2"
      name="UC1_QS2"
      response="Resp2"/>
  <tactics name="Increase_Available_Resources"
      qAttribute="PERFORMANCE"/>
  <tactics name="Mantain_Multiple_Copies"
      qAttribute="PERFORMANCE"/>
  <tactics name="Introduce_Concurrency"
      qAttribute="PERFORMANCE"/>
  <tactics name="Increase_Computation_Efficiency"
      qAttribute="PERFORMANCE"/>
  <tactics name="Reduce_Computational_Overhead"
      qAttribute="PERFORMANCE"/>
  <tactics name="Manage_Event_Rate"
      qAttribute="PERFORMANCE"/>
  <tactics name="Control_Frequency_of_Sampling"
      qAttribute="PERFORMANCE"/>
  <tactics name="Scheduling_Policy"
      qAttribute="PERFORMANCE"/>
  <tactics name="Runtime_Registration"
      qAttribute="MODIFIABILITY"/>
  <analyzedQs
      qs="UC1_QS1"
      name="A_UC1_QS1">
    <decisions
        name="Dec1"
        rationale="Rational D1"
        code="Dec1"
        appliedTactics="//@tactics.2"
