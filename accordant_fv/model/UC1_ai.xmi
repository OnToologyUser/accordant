  <analyzedQs
      qs="UC1_QS2"
      name="A_UC1_QS2">
    <decisions
        name="Dec2"
        rationale="Rationale D2"
        code="Dec2"
        appliedTactics="//@tactics.6"
        isSensitivityPoint="true"/>
  </analyzedQs>
  <constraints
      code="UC1-OpenSource"
      type="LICENSE"
      value="OPENSOURCE"/>
  <constraints
      code="CloudDeployment"
      type="DEPLOYMENT_MODEL"
      value="CLOUD"/>
  <constraints
      code="BATCH"
      type="PROC_MODEL"
      value="BATCH"/>
  <constraints
      code="procsModelStream"
      type="PROC_MODEL"
      value="STREAM"/>
